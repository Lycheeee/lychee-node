  },
  "homepage": "https://github.com/lycheeee/lychee-node#readme",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.6.5",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^7.0.0",
    "eslint-plugin-import": "^1.4.0",
